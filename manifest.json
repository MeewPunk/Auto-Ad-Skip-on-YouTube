{
    "name": "Auto Ad Skip on YouTube",
    "version": "1.0.0",
    "description": "Auto Ad Skip on YouTube",
    "manifest_version": 3,
    "author": "dev 2077",
    "action":{
        "default_popup": "index.html",
        "default_title": "Latest Covid Report"
    },
    "background": {
        "service_worker": "script.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ]
}